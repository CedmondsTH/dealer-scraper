{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "streamlit run run.py --server.port=$PORT --server.address=0.0.0.0 --server.headless=true --server.enableCORS=false",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300
  }
} 